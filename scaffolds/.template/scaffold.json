{
	"i18n": true,
	"typescript": true,
	"auth": false,
	"store": true,
	"mock": true,
	"pkgData": {
		"name": "@icedesign/antd-design-pro",
		"version": "0.2.5",
		"description": "使用 TypeScript，包含大量 UI 区块，比如图表、表单等。",
		"dependencies": {
			"ahooks": "^2.0.0",
			"@antv/data-set": "^0.10.2",
			"bizcharts": "^3.5.6",
			"antd": "^4.12.3"
		},
		"devDependencies": {
			"build-plugin-fusion-material": "^0.1.5"
		},
		"scripts": {
			"design": "icejs build --design",
			"prepublishOnly": "npm run build -- --design && npm run screenshot"
		},
		"repository": {
			"type": "git",
			"url": "https://github.com/ice-lab/antd-materials/tree/master/scaffolds/antd-scaffold"
		},
		"scaffoldConfig": {
			"name": "antd Design Pro - TS",
			"title": "antd Design Pro - TS",
			"category": "Basic",
			"screenshot": "https://img.alicdn.com/tfs/TB1jzt1YEz1gK0jSZLeXXb9kVXa-1349-833.png",
			"views": [
				{
					"title": "分析页面",
					"path": "#/dashboard/analysis",
					"screenshot": "",
					"html": ""
				}
			]
		}
	},
	"build": {
		"theme": {
			"package": "build-plugin-antd",
			"version": "^0.6.2",
			"themeConfig": {
				"primary-color": "#ccc"
			  }
		},
		"buildConfig": {
			"plugins": [
				"build-plugin-fusion-material"
			]
		}
	},
	"layouts": [
		{
			"type": "builtIn",
			"name": "BasicLayout",
			"menuConfig": true,
			"layoutConfig": {
				"shell": {
					"nav": {
						"name": "PageNav",
						"type": "builtIn"
					},
					"navHoz": [
						{
							"name": "GlobalSearch",
							"type": "builtIn"
						}
					],
					"action": [
						{
							"name": "Notice",
							"type": "builtIn"
						},
						{
							"name": "SolutionLink",
							"type": "builtIn"
						},
						{
							"name": "HeaderAvatar",
							"type": "builtIn"
						}
					],
					"branding": {
						"name": "Logo",
						"props": {
							"image": "https://img.alicdn.com/tfs/TB1.ZBecq67gK0jSZFHXXa9jVXa-904-826.png",
							"text": "Logo"
						},
						"type": "builtIn"
					},
					"footer": {
						"name": "Footer",
						"type": "builtIn"
					}
				}
			}
		},
		{
			"name": "UserLayout",
			"type": "builtIn"
		}
	],
	"menu": {
		"asideMenu": [
			{
				"name": "数据页面",
				"path": "/",
				"icon": "chart-pie",
				"children": [
					{
						"name": "分析页面",
						"path": "/dashboard/analysis"
					},
					{
						"name": "监控页面",
						"path": "/dashboard/monitor"
					},
					{
						"name": "工作台",
						"path": "/dashboard/workplace"
					}
				]
			},
			{
				"name": "表单页面",
				"path": "/",
				"icon": "copy",
				"children": [
					{
						"name": "单列表单",
						"path": "/form/basic"
					},
					{
						"name": "两列表单",
						"path": "/form/two"
					},
					{
						"name": "三列表单",
						"path": "/form/three"
					},
					{
						"name": "四列表单",
						"path": "/form/four"
					},
					{
						"name": "分步表单",
						"path": "/form/step"
					},
					{
						"name": "分类表单",
						"path": "/form/classified"
					},
					{
						"name": "分组表单",
						"path": "/form/group"
					},
					{
						"name": "流程表单",
						"path": "/form/flow"
					},
					{
						"name": "分级表单",
						"path": "/form/hierarchical"
					}
				]
			},
			{
				"name": "列表页面",
				"path": "/",
				"icon": "chart-bar",
				"children": [
					{
						"name": "基础列表",
						"path": "/list/basic"
					},
					{
						"name": "卡片列表",
						"path": "/list/card"
					},
					{
						"name": "表格列表",
						"path": "/",
						"children": [
							{
								"name": "基础过滤",
								"path": "/list/table/filter"
							},
							{
								"name": "单列过滤",
								"path": "/list/table/singlecol"
							},
							{
								"name": "多列过滤",
								"path": "/list/table/mutilcol"
							},
							{
								"name": "带操作列",
								"path": "/list/table/action"
							},
							{
								"name": "可展开表",
								"path": "/list/table/expand"
							},
							{
								"name": "单层树表",
								"path": "/list/table/singletree"
							},
							{
								"name": "弹窗表格",
								"path": "/list/table/dialog"
							},
							{
								"name": "合并单元格",
								"path": "/list/table/mergecell"
							}
						]
					}
				]
			},
			{
				"name": "详情页面",
				"path": "/",
				"icon": "calendar",
				"children": [
					{
						"name": "基础详情",
						"path": "/detail/basic"
					},
					{
						"name": "高级详情",
						"path": "/detail/advanced"
					}
				]
			},
			{
				"name": "结果&缺省",
				"path": "/",
				"icon": "warning",
				"children": [
					{
						"name": "成功页面",
						"path": "/feedback/success"
					},
					{
						"name": "失败页面",
						"path": "/feedback/fail"
					},
					{
						"name": "403",
						"path": "/feedback/403"
					},
					{
						"name": "404",
						"path": "/feedback/404"
					},
					{
						"name": "500",
						"path": "/feedback/500"
					}
				]
			},
			{
				"name": "设置页面",
				"path": "/",
				"icon": "set",
				"children": [
					{
						"name": "系统设置",
						"path": "/settings"
					},
					{
						"name": "个人设置",
						"path": "/person"
					}
				]
			},
			{
				"name": "登录&注册",
				"path": "/",
				"icon": "account",
				"children": [
					{
						"name": "登录",
						"path": "/user/login"
					},
					{
						"name": "注册",
						"path": "/user/register"
					}
				]
			}
		],
		"headerMenu": []
	},
	"routers": [
		{
			"path": "/",
			"component": "BasicLayout",
			"children": [
				{
					"path": "/dashboard/analysis",
					"page": {
						"name": "Analysis",
						"blocks": {
							"container": "block.common",
							"packages": [
								{
									"name": "@icedesign/antd-page-header",
									"type": "common",
									"props": {
										"title": "分析页面",
										"breadcrumbs": [
											{
												"name": "Dashboard"
											},
											{
												"name": "分析页面"
											}
										]
									}
								},
								[
									"@icedesign/antd-card-bar-chart",
									"@icedesign/antd-card-area-chart",
									"@icedesign/antd-card-typebar-chart",
									"@icedesign/antd-card-line-chart"
								],
								"@icedesign/antd-card-rank-chart",
								[
									{
										"span": 4,
										"name": "@icedesign/antd-card-pie-chart"
									},
									{
										"span": 8,
										"name": "@icedesign/antd-card-group-bar-chart"
									}
								]
							]
						}
					}
				}
			]
		}
	]
}